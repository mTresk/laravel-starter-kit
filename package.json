{
  "name": "laravel-starter",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s fix-all build:vite",
    "build:vite": "vite build",
    "fonts": "tsx ./src/utils/tasks/fonts.ts",
    "lint:js": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,pcss}\" \"resources/**/*.{css,scss,pcss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss,pcss}\" \"resources/**/*.{css,scss,pcss}\" --fix",
    "px-to-rem": "tsx ./src/utils/tasks/px-to-rem.ts",
    "lint": "run-s lint:js lint:css",
    "lint:fix": "run-s lint:js:fix lint:css:fix",
    "fix-all": "run-s px-to-rem lint:fix"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "inputmask": "^5.0.9",
    "js-datepicker": "^5.18.4",
    "lightgallery": "^2.9.0",
    "nice-select2": "^2.4.1",
    "nouislider": "^15.8.1",
    "ofetch": "^1.5.1",
    "smooth-scrollbar": "^8.8.4",
    "swiper": "^12.0.3",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@tailwindcss/vite": "^4.1.16",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.0",
    "eslint-plugin-format": "^1.0.2",
    "fontmin": "^2.0.3",
    "glob": "^11.0.3",
    "js-beautify": "^1.15.4",
    "laravel-vite-plugin": "^2.0.1",
    "npm-run-all2": "^8.0.4",
    "sass": "^1.93.3",
    "sharp": "^0.34.4",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vue-tsc": "^3.1.2"
  }
}
